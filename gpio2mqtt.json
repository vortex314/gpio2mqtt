{
    "mqtt": {
        "connection": "tcp://localhost",
        "keepAliveInterval": 5,
        "device": "gpio",
        "LW": {
            "topic": "src/gpio/system/alive",
            "message": "false",
            "qos": 1,
            "retain": true
        }
    },
    "gpio": {
        "2": [
            "OUTPUT",
            1
        ],
        "22": [
            "OUTPUT",
            0
        ],
        "26": [
            "OUTPUT",
            0
        ]
    },
    "log": {
        "level": "I",
        "file": "gpio2mqtt.log"
    }
}